        <KeysToAdd>
          <RegistryKey>
            <KeyName>System\Platform\DeviceTargetingInfo</KeyName>
            <Subkeys/>
            <Values>
              <RegistryKeyValue>
                <Name>PhoneManufacturer</Name>
                <Value>NOKIA</Value>
                <ValueType>1</ValueType>
